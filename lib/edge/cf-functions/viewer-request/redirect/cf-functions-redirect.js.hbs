  var SOURCE_URI = '{{ webHostingSpec.redirect.source }}';
  var SOURCE_URI_RE = new RegExp(SOURCE_URI);
  var TARGET_URI = '{{ webHostingSpec.redirect.target }}';

  if (request.uri.match(SOURCE_URI_RE)) {
    return {
      statusCode: 301,
      statusDescription: 'Moved Permanently',
      headers: {
        location: { value: TARGET_URI },
      },
    };
  }
